{
  "name": "propertee",
  "displayName": "ProperTee",
  "description": "Syntax highlighting and formatting for the ProperTee scripting language",
  "version": "0.2.0",
  "main": "./extension.js",
  "publisher": "flatide",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/flatide/ProperTee.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onLanguage:propertee"
  ],
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "contributes": {
    "languages": [
      {
        "id": "propertee",
        "aliases": [
          "ProperTee",
          "propertee"
        ],
        "extensions": [
          ".pt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "propertee",
        "scopeName": "source.propertee",
        "path": "./syntaxes/propertee.tmLanguage.json"
      }
    ]
  }
}
